{
    "name": "gpupo/brazilian-banking-automation",
    "description": "CNAB tool",
    "homepage": "https://opensource.gpupo.com/brazilian-banking-automation/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["cnab", "cnab400", "brazilian", "banking", "php"],
    "license": "MIT",
    "autoload": {
        "psr-4": { "Gpupo\\BrazilianBankingAutomation\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\BrazilianBankingAutomation\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "gpupo/common": "^2.7",
        "symfony/console": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/yaml": "^4.0",
        "gpupo/common-sdk": "^4.3",
        "andersondanilo/cnab_php": "^1.3"
    }
}
